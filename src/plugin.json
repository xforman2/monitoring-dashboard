{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Server Monitoring",
  "id": "muni-servermonitoring-app",
  "info": {
    "keywords": ["app"],
    "description": "",
    "author": {
      "name": "Muni"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "Home",
      "path": "/a/%PLUGIN_ID%/home",
      "role": "Admin",
      "addToNav": true,
      "defaultNav": true
    },
    {
      "type": "page",
      "name": "Gpu Usage",
      "path": "/a/%PLUGIN_ID%/gpu-usage",
      "role": "Admin",
      "addToNav": true,
      "defaultNav": false
    },
    {
      "type": "page",
      "name": "CPU",
      "path": "/a/%PLUGIN_ID%/cpu",
      "role": "Admin",
      "addToNav": true,
      "defaultNav": false
    },
    {
      "type": "page",
      "name": "RAM",
      "path": "/a/%PLUGIN_ID%/ram",
      "role": "Admin",
      "addToNav": true,
      "defaultNav": false
    },
    {
      "type": "page",
      "name": "Disk",
      "path": "/a/%PLUGIN_ID%/disk",
      "role": "Admin",
      "addToNav": true,
      "defaultNav": false
    }, 
    {
      "type": "page",
      "name": "Process Details",
      "path": "/a/%PLUGIN_ID%/process-details",
      "role": "Admin",
      "addToNav": true,
      "defaultNav": false
    }

    
  ],
  "dependencies": {
    "grafanaDependency": ">=10.0.3",
    "plugins": []
  }
}
